<div class="center hero-unit">
<h1>仓库管理系统</h1>
</div>

<div class="row">
  <div class="span6 offset3">
    <%=form_for(:session,url:sessions_path) do|f| %>
        <%=f.label :"姓名"%>
        <%=f.text_field :name%>
        <%=f.label :"密码"%>
        <%=f.password_field :password%>
        <%=f.submit"登陆",class:"btn btn-large btn-primary"%>
    <%end%>
    <p><%=link_to "注册",signup_path%></p>
  </div>
</div>